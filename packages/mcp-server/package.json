{
  "name": "@dockflow-tools/mcp",
  "version": "1.0.1",
  "description": "MCP server for Dockflow documentation - enables AI assistants to search and query Dockflow docs",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "dockflow-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "release": "npm version patch --no-git-tag-version && npm run build && npm publish --access public"
  },
  "keywords": [
    "dockflow",
    "mcp",
    "model-context-protocol",
    "ai",
    "documentation"
  ],
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.27.1",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  }
}
